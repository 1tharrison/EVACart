<body class="l-body headertype_sticky headerpos_top hometype_fullscreen">
    <div class='l-preloader'></div>
    <!-- HEADER -->
    <div class="l-header">
        <div class="l-header-h">
            <!-- subheader -->
            <div class="l-subheader">
                <div class="l-subheader-h i-cf">

                    <!-- logo -->
                    <div class="w-logo">
                        <div class="w-logo-h">
                            <a class="w-logo-link" href="#">
                                <img class="w-logo-img" src="~/Content/Images/img/placeholder/evac_logo2.jpg" alt="EVAC">
                                <span class="w-logo-title">
                                    <span class="w-logo-title-h">CORSA</span>
                                </span>
                            </a>
                        </div>
                    </div>

                    <!-- socials -->
                    <div class="w-socials size_normal">
                        <div class="w-socials-h">
                            <div class="w-socials-list">
                                <div class="w-socials-item facebook">
                                    <a class="w-socials-item-link" target="_blank" href="https://www.facebook.com/pages/The-Endowment-for-Veterans-Art-Campaign/602716183129772">
                                        <i class="fa fa-facebook"></i>
                                    </a>
                                    <div class="w-socials-item-popup">
                                        <div class="w-socials-item-popup-h">
                                            <span class="w-socials-item-popup-text">Facebook</span>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>

                    <!-- nav -->
                    <nav class="w-nav">
                        <div class="w-nav-h">
                            <div class="w-nav-control">
                                <i class="fa fa-bars"></i>
                            </div>
                            <div class="w-nav-list layout_hor width_auto level_1">
                                <div class="w-nav-list-h">
                                    <div class="w-nav-item level_1 with_sublevel active">
                                        <div class="w-nav-item-h">
                                            <div class="w-nav-anchor level_1"><span class="w-nav-title"></span><span class="w-nav-hint"></span></div>@Html.ActionLink("Home", "Index", "Home")    &nbsp;&nbsp;
                                        </div>
                                    </div>

                                    <div class="w-nav-item level_1">
                                        <div class="w-nav-item-h">
                                            <a class=" w-nav-anchor level_1"><span class="w-nav-title"></span><span class="w-nav-hint"></span></a>@Html.ActionLink("About", "About", "Home")&nbsp;&nbsp;
                                        </div>
                                    </div>


                                    <div class="w-nav-item level_1">
                                        <div class="w-nav-item-h">
                                            <a class="w-nav-anchor level_1"><span class="w-nav-title"></span><span class="w-nav-hint"></span></a>@Html.ActionLink("Artists", "Index", "EvacArtists")&nbsp;&nbsp;
                                        </div>
                                    </div>

                                    <div class="w-nav-item level_1">
                                        <div class="w-nav-item-h">
                                            <a class="w-nav-anchor level_1"><span class="w-nav-title"></span><span class="w-nav-hint"></span></a>@Html.ActionLink("Connect", "Contact", "Home")&nbsp;&nbsp;
                                        </div>

                                    </div>
                                    <div class="w-nav-item level_1">
                                        <div class="w-nav-item-h">
                                            <a class="w-nav-anchor level_1"><span class="w-nav-title"></span><span class="w-nav-hint"></span></a>   @Html.Partial("_LoginPartial")&nbsp;&nbsp;
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </div>
    <!-- /HEADER -->



    <hr />
    <br/>
    @RenderBody()

    <a class="w-toplink" href="#"><i class="fa fa-chevron-up"></i></a>


